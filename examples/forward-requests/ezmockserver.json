{
  "logLevel": "DEBUG",
  "api": {
    "httpPort": 3050
  },
  "server": {
    "httpPort": 3000
  },
  "sessionsDirectory": "./sessions",
  "proxy": {
    "prefix": [{ "path": "/jsonplaceholder/", "proxyPass": "https://jsonplaceholder.typicode.com", "rewrite": "/" }]
  },
  "defaultSession": {
    "name": "my-forwarded-session",
    "fileType": "content",
    "countMode": "COUNT_ALL"
  }
}
